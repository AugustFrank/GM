@model GM011.Models.Costumer
@{
    ViewData["Title"] = "Contact";
}
<h2>@ViewData["Title"].</h2>
<h3>@ViewData["Message"]</h3>


<div class="col-md-4 col-md-offset-2">
    <h4 class="rowPadding"> We will contact you within 24 hours</h4> @*--der mangler noget styling--*@
    <div class="col-md-12 form-group">
        <div class="col-md-6 form-group">
            <input type="text" class="form-control" id="exampleInputName" placeholder="Name">
        </div>
        <div class="col-md-6 form-group">
            <input type="email" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="Enter @Html.DisplayNameFor(Model=>Model.Email)">
            <small id="emailHelp" class="form-text text-muted">We'll never share your email with anyone else.</small>
        </div>

        <div class="">
            <div class="col-md-6 form-group">
                <input type="text" class="form-control" id="exampleInputName" placeholder="Pickup date">
            </div>
            <div class="col-md-6 form-group">
                <input type="text" class="form-control" id="exampleInputName" placeholder="Pickup time">
            </div>
        </div>
        <div class="">
            <div class="col-md-6">
                <input type="text" class="form-control" id="exampleInputName" placeholder="Return date">
            </div>
            <div class="col-md-6">
                <input type="text" class="form-control" id="exampleInputName" placeholder="Return time">
            </div>
        </div>
        <div class="col-md-12 form-group">
            <label for="exampleTextarea">Message</label>
            <textarea class="form-control" id="exampleTextarea" rows="3"></textarea>
        </div>
    </div>
</div>







<address>
    One Microsoft Way<br />
    Redmond, WA 98052-6399<br />
    <abbr title="Phone">P:</abbr>
    425.555.0100
</address>

<address>
    <strong>Support:</strong> <a href="mailto:Support@example.com">Support@example.com</a><br />
    <strong>Marketing:</strong> <a href="mailto:Marketing@example.com">Marketing@example.com</a>
</address>
